{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "Common": {
        "class": "Tenant",
        "firewallAddressList1": {
            "class": "FirewallAddressList",
            "addresses": [
                "192.0.2.10",
                "192.1.2.0/24"
            ]
        },
        "firewallPortList1": {
            "class": "FirewallPortList",
            "ports": [
                "8080",
                "8888"
            ]
        },
        "firewallPortList2": {
            "class": "FirewallPortList",
            "ports": [
                "8123",
                "8234",
                "8300-8350"
            ]
        },
        "ManagementIpFirewall": {
            "class": "ManagementIpFirewall",
            "remark": "management IP firewall description",
            "rules": [
                {
                    "name": "firewallRule",
                    "remark": "firewall rule description",
                    "action": "reject",
                    "protocol": "tcp",
                    "loggingEnabled": true,
                    "source": {
                        "addressLists": [
                            "/Common/firewallAddressList1"
                        ],
                        "portLists": [
                            "/Common/firewallPortList1",
                            "/Common/firewallPortList2"
                        ]
                    },
                    "destination": {
                        "addressLists": [
                            "/Common/firewallAddressList1"
                        ],
                        "portLists": [
                            "/Common/firewallPortList1",
                            "/Common/firewallPortList2"
                        ]
                    }
                }
            ]
        }
    }
}
