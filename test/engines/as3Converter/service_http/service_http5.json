{
    "class": "ADC",
    "updateMode": "selective",
    "schemaVersion": "3.22.0",
    "id": "declarationId",
    "label": "theDeclaration",
    "remark": "All properties declaration",
    "AS3_Tenant": {
        "class": "Tenant",
        "AS3_Application": {
            "class": "Application",
            "template": "http",
            "serviceMain": {
                "class": "Service_HTTP",
                "iRules": [
                    {
                        "use": "/AS3_Tenant/AS3_Application/Maintenance_iRule"
                    }
                ],
                "virtualAddresses": [
                    "10.0.1.10",
                    "10.0.1.11"
                ],
                "pool": "web_pool"
            },
            "web_pool": {
                "class": "Pool",
                "monitors": [
                    {
                        "bigip": "/Common/http"
                    }
                ],
                "members": [
                    {
                        "addressDiscovery": "static",
                        "servicePort": 80,
                        "serverAddresses": [
                            "192.0.1.170",
                            "192.0.1.171"
                        ],
                        "shareNodes": true
                    }
                ]
            },
            "Maintenance_iRule": {
                "class": "iRule",
                "iRule": {
                    "base64": ""
                }
            }
        }
    }
}
