{
    "class": "ADC",
    "updateMode": "selective",
    "schemaVersion": "3.8.0",
    "id": "declarationId",
    "label": "theDeclaration",
    "remark": "All properties declaration",
    "AS3_Tenant": {
        "class": "Tenant",
        "AS3_Application": {
            "class": "Application",
            "template": "udp",
            "serviceMain": {
                "class": "Service_UDP",
                "virtualAddresses": [
                    "10.0.5.10"
                ],
                "virtualPort": 1344,
                "pool": "svc_pool"
            },
            "svc_pool": {
                "class": "Pool",
                "monitors": [{
                    "use": "/AS3_Tenant/AS3_Application/udp_monitor"
                }],
                "members": [{
                    "addressDiscovery": "static",
                    "servicePort": 1344,
                    "serverAddresses": [
                      "192.0.5.10",
                      "192.0.5.11"
                    ],
                    "shareNodes": true
                }]
            },
            "udp_monitor": {
                "class": "Monitor",
                "monitorType": "udp",
                "send": "OPTIONS icap://icap.example.test/ ICAP/1.0\r\nUser-Agent: f5-ADC\r\n\r\n",
                "receive": "ICAP/1.0 200 OK",
                "adaptive": false
            }
        }
    }
}
