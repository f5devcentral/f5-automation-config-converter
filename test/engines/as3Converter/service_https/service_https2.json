{
    "class": "ADC",
    "updateMode": "selective",
    "schemaVersion": "3.8.0",
    "id": "declarationId",
    "label": "theDeclaration",
    "remark": "All properties declaration",
    "AS3_Tenant": {
        "class": "Tenant",
        "AS3_Application": {
            "class": "Application",
            "template": "https",
            "serviceMain": {
                "class": "Service_HTTPS",
                "virtualAddresses": [
                    "1.1.1.1"
                ],
                "profileHTTP": {
                    "use": "/AS3_Tenant/AS3_Application/AS3_http"
                },
                "profileHTTPCompression": {
                    "use": "/AS3_Tenant/AS3_Application/AS3_compress"
                },
                "profileHTTPAcceleration": {
                    "use": "/AS3_Tenant/AS3_Application/AS3_accel"
                },
                "profileMultiplex": {
                    "use": "/AS3_Tenant/AS3_Application/AS3_multiplex"
                },
                "profileAnalytics": {
                    "use": "/AS3_Tenant/AS3_Application/AS3_analytics"
                },
                "serverTLS": "webtls"
            },
            "AS3_http": {
                "class": "HTTP_Profile"
            },
            "AS3_compress": {
                "class": "HTTP_Compress"
            },
            "AS3_accel": {
                "class": "HTTP_Acceleration_Profile"
            },
            "AS3_multiplex": {
                "class": "Multiplex_Profile"
            },
            "AS3_analytics": {
                "class": "Analytics_Profile"
            },
            "webtls": {
                "class": "TLS_Server",
                "certificates": [
                    {
                        "certificate": "/AS3_Tenant/AS3_Application/webcert"
                    }
                ]
            },
            "webcert": {
                "class": "Certificate",
                "remark": "in practice we recommend using a passphrase",
                "certificate": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----",
                "privateKey": "",
                "passphrase": {
                    "ciphertext": "ZjVmNQ==",
                    "protected": "eyJhbGciOiJkaXIiLCJlbmMiOiJub25lIn0"
                }
            }
        }
    }
}
