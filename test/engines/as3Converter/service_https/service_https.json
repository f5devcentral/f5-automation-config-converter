{
    "class": "ADC",
    "updateMode": "selective",
    "schemaVersion": "3.8.0",
    "id": "declarationId",
    "label": "theDeclaration",
    "remark": "All properties declaration",
    "AS3_Tenant": {
        "class": "Tenant",
        "AS3_Application": {
            "class": "Application",
            "template": "https",
            "serviceMain": {
                "class": "Service_HTTPS",
                "virtualAddresses": [
                    "192.0.2.11"
                ],
                "pool": "web_pool",
                "serverTLS": "webtls"
            },
            "web_pool": {
                "class": "Pool",
                "loadBalancingMode": "predictive-node",
                "monitors": [
                    {
                        "bigip": "/Common/http"
                    }
                ],
                "members": [
                    {
                        "addressDiscovery": "static",
                        "servicePort": 80,
                        "serverAddresses": [
                            "192.0.2.12",
                            "192.0.2.13"
                        ],
                        "shareNodes": true
                    }
                ]
            },
            "webtls": {
                "class": "TLS_Server",
                "certificates": [
                    {
                        "certificate": "/AS3_Tenant/AS3_Application/webcert"
                    }
                ]
            },
            "webcert": {
                "class": "Certificate",
                "remark": "in practice we recommend using a passphrase",
                "certificate": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----",
                "privateKey": "",
                "passphrase": {
                    "ciphertext": "ZjVmNQ==",
                    "protected": "eyJhbGciOiJkaXIiLCJlbmMiOiJub25lIn0"
                }
            }
        }
    }
}
